{
  "expo": {
    "name": "Palura TTS",
    "slug": "palura-tts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.palura-tts"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.paluratts"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [],
    "experiments": {
      "typedRoutes": false
    }
  }
}
